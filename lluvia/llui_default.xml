<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote name="lluvia"
          fetch="https://github.com/LLuviaOS"
          revision="LL-4.X" />

  <remote name="devices"
          fetch="https://github.com/LLuviaDevices"
          revision="LL-4.X" />

  <remote name="lluvia_gitlab"
          fetch="https://gitlab.com/LLuviaOpenSource"
          revision="LL-4.X" />
  
   <remote name="los"
          fetch="https://github.com/LineageOS"
          revision="lineage-17.0" />

  <!-- LLUVIA -->
  <project path="manifest" name="android_manifest" remote="lluvia" />
  <project path="build/blueprint" name="Android_build_blueprint" groups="pdk,tradefed" remote="lluvia" />
  <project path="build/make" name="android_build" remote="lluvia" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="android_build_soong" groups="pdk,tradefed" remote="lluvia" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

